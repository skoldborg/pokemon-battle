{
  "name": "pokemon-battle",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "www-server",
    "www-client"
  ],
  "scripts": {
    "dev:server": "yarn workspace www-server dev",
    "dev:client": "yarn workspace www-client dev",
    "test:server": "yarn workspace www-server test",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
